{% extends "layout.njk" %}
{% block content %}
<body>
	<main class="dark">
    <div class="dark-container">
      <div class="hmain-container">  
        <div class="hbody-container">
          <div class="hinner-container">        
              <div class="hbody-header">
                <div>
                  <h2>Andrew Obatake</h2>
                  <h3>Computer Engineering</h3>
                  <h3>Hawaii</h3>
                </div>
                <div class="hsocial-container">
                  <a href="https://github.com/aobatake">{% include "github.njk" %}</a>
